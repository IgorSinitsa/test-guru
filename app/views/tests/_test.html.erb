 <tr>
 <td> <%= link_to "#{test.title}", test_path(test) %> </td>
 <td> <%= test.level %> </td> 
 <td> <%= test.questions.count %> </td>
 <td> <%= button_to "Edit", edit_test_path(test), method: :get %> </td>
 <td> <%= button_to "Delete", test_path(test), method: :delete,
                                               remote: :true,
                                               data: { confirm: "Are you sure?" } %></td>
</tr>