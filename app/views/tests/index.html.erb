<h1><%= t(".header") %></h1>
<table class="table table-striped">
  <thead class="thead-dark">
    <tr >
      <th class="sort-by-title pointer" ><%= t(".title") %>
        <i class="bi bi-arrow-down-square-fill "></i>
        <i class="bi bi-arrow-up-square-fill hide" ></i> </th>
      <th><%= t(".level") %>  </th>
      <th><%= t(".questions") %></th>
      <th ><%= t(".actions") %></th>
    </tr>
  </thead>
  <tbody>
    <%= render @tests %>
  </tbody>
</table>
