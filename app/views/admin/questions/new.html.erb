<h1>New Question</h1>
<h1><%= question_header(@question.test) %></h1>
<%= form_with model: [:admin, @test,  @question], local: true do |form| %>
  <%= render "shared/errors", object: @question %>
  <p>
    <%= form.label :body %>
    <%= form.text_area :body, id: :question_body, rows: 7, cols: 60 %>
  </p>
  <br>
  <%= form.submit t('feedback.send'), class: 'btn btn-outline-primary' %>
<% end %>
