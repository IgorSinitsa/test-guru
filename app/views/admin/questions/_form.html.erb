<%= form_with model: [:admin,  @question], local: true do |form| %>
  <%= render "shared/errors", object: @question %>
  <p>
    <%= form.label :body %>
    <%= form.text_area :body, id: :question_body, rows: 7, cols: 60 %>
  </p>
  <br>
  <%= form.submit  class: 'btn btn-outline-primary' %>
<% end %>
